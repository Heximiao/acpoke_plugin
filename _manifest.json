{
  "manifest_version": 1,
  "name": "戳一戳插件",
  "display_name": "戳一戳插件",
  "version": "0.4.0",
  "description": "一个主动发起“戳一戳”的 MaiMai 插件，帮助你轻松与好友互动，自动发起戳一戳，增添群聊乐趣。",
  "author": {
    "name": "何夕",
    "url": "https://github.com/liuchunyu44/acpoke_plugin",
    "email": "lcy1416603026@gmail.com"
  },
  "license": "MIT",
  "host_application": {
    "min_version": "0.9.0",
    "max_version": "0.10.1"
  },
  "entry": "plugin.py",
  "homepage_url": "https://github.com/liuchunyu44/acpoke_plugin",
  "repository_url": "https://github.com/liuchunyu44/acpoke_plugin",
  "keywords": ["poke", "maimai", "互动", "自动戳人", "插件"],
  "categories": ["Fun", "Interaction", "Automation"],
  "default_locale": "zh-CN",
  "locales_path": "_locales",
  "plugin_info": {
    "is_built_in": false,
    "plugin_type": "chat",
    "components": [
      {
        "type": "action",
        "name": "ActivePokeAction",
        "description": "主动模式下，插件可以自动戳一戳群成员，增强群聊互动体验"
      }
    ],
    "features": ["戳一戳", "自动互动"]
  }
}
